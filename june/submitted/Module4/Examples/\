#include <cstdio>
#include <cstring>
#include <iostream>
using namespace std;

int main() {
  char s1[80], s2[80];
  strcpy(s1, "We are learning C++");
  strcpy(s2, ", general-purpose programming language!");
  cout << "lengths:" << endl;
  cout << "\ts1: " << strlen(s1) << endl;
  cout << "\ts2: " << strlen(s2) << endl;

  if (strcmp(s1, s2))
    cout << "the two strings are equal!";

  return 0;
}
