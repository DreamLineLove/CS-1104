#include <iostream>
using namespace std;

int main() {
  string people[] = {"Min Thu", "Cherry", "Thura", "Thar Htoo", ""};
  int length = sizeof(people) / sizeof(people[0]);

  cout << "Original array..." << endl;
  for (int i = 0; i < length; i++) {
    cout << "\t" << people[i];
  }

  for (int i = 3; i > 1; i++) {
    people[i + 1] = people[i];
  }
  people[2] = "Thiha";
  cout << "After inserting \"Thiha\"..." << endl;
  for (int i = 0; i < length; i++) {
    cout << "\t" << people[i];
  }

  // cout << "\n"
}
