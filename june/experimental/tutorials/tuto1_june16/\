#include <iostream>
using namespace std;

void insertPerson(string array[], int length, string name, int index) {
  for (int i = index; i < length; i++) {
    string temp = array[i];
    array[i] = array[i + 1];
    array[i + 1] = temp;
  }
}

int main() {
  string people[5] = {
      "Min Thu", "Cherry", "Thura", "Thar Htoo", "",
  };
  cout << "Original array:" << endl;
  for (int i = 0; i < 5; i++) {
    cout << "\t" << people[i] << endl;
  }
  insertPerson(people, 5, "Thiha", 2);

  return 0;
}
