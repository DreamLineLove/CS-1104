#include <fstream>
#include <iostream>
#include <regex>
using namespace std;

int main() {
  regex pattern("(fe)?male", regex_constants::icase);

  string name;
  cout << "Enter name of the file: ";
  cin >> name;

  string line;
  ifstream file(name);

  while (getline(file, line)) {
    cout << line << endl;
  }

  file.close();

  return 0;
}
